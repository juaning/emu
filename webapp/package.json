{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://api:3000",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "ajv": "^6.7.0",
    "chartist": "^0.11.0",
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "history": "^4.7.2",
    "is-url": "^1.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-big-calendar": "^0.19.2",
    "react-bootstrap-sweetalert": "^4.4.1",
    "react-chartist": "^0.13.2",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.7.0",
    "react-google-maps": "^9.4.5",
    "react-jvectormap": "0.0.10",
    "react-nouislider": "^2.0.1",
    "react-number-format": "^4.0.8",
    "react-popper": "^1.3.3",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.5.1",
    "react-swipeable-views": "^0.12.18",
    "react-table": "6.8.6",
    "react-tagsinput": "^3.19.0",
    "react-tap-event-plugin": "^3.0.3",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.2",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "PORT=8080 react-scripts start",
    "start-new": "npm-run-all -p watch-css start-js",
    "start": "PORT=8080 react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-old": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build-old"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
